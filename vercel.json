{
    "version": 2,
    "rewrites": [
        { "source": "/promo", "destination": "/promo.html" },
        {
            "source": "/calculadora-custo-impressao-3d",
            "destination": "/calculadora-custo-impressao-3d.html"
        },
        { "source": "/gestao-de-filamentos", "destination": "/gestao-de-filamentos.html" },
        {
            "source": "/gestao-pedidos-impressao-3d",
            "destination": "/gestao-pedidos-impressao-3d.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Credentials", "value": "true" },
                { "key": "Access-Control-Allow-Origin", "value": "*" },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
                }
            ]
        }
    ]
}
